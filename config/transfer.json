{
    "debug": true,
    "minStep": 30,
    "http": {
        "enabled": true,
        "listen": "%%TRANSFER_HTTP%%"
    },
    "rpc": {
        "enabled": true,
        "listen": "%%TRANSFER_RPC%%"
    },
    "socket": {
        "enabled": true,
        "listen": "0.0.0.0:4444",
        "timeout": 3600
    },
    "judge": {
        "enabled": true,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "replicas": 500,
        "cluster": {
            "judge-00" : "%%JUDGE_RPC%%"
        }
    },
    "graph": {
        "enabled": true,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "replicas": 500,
        "cluster": {
            "graph-00" : "%%GRAPH_RPC%%"
        }
    },
    "tsdb": {
        "enabled": false,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "retry": 3,
        "address": "127.0.0.1:8088"
    },
    "ims": {
        "enabled": true,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "retry": 3,
        "period": 60,
        "address": "%%TRANSFER_IMS_URL%%", 
        "falconToIms":{
            "cup.busy": {
                "name":"cup"
            },
            "load.1min": {
                "name":"cpu_min1",
                "expression":"value*100"
            },
            "load.5min": {
                "name":"cpu_min5",
                "expression":"value*100"
            },
            "mem.memused": {
                "name":"mem_app_total",
                "expression":"value/1048576"
            },
            "mem.swaptotal":{
                "name":"mem_swap_total",
                "expression":"value/1024"
            },
            "mem.swapused": {
                "name":"mem_swap_used",
                "expression":"value/1024"
            },
            "mem.memtotal": {
                "name":"mem_total",
                "expression":"value/1024"
            },
            "df.bytes.used.percent":{
                "name":"disk_used_rate",
                "expression":"value*100",
                "tag":"mount"
            },
            "disk.io.util":{
                "name":"disk_util_max",
                "expression":"value*100",
                "tag":"device"
            }, 
            "net.if.in.dropped": {
                "name":"net_in_discard",
                "tag":"iface"
            }, 
            "net.if.in.errors":{
                "name":"net_in_err",
                "tag":"iface"
            }, 
            "net.if.in.bytes":{
                "name":"net_in_size",
                "expression":"value/1024",
                "tag":"iface"
            }, 
            "net.if.out.dropped":{
                "name":"net_out_discard",
                "tag":"iface"
            }, 
            "net.if.out.errors":{
                "name":"net_out_err",
                "tag":"iface"
            }, 
            "net.if.out.bytes": {
                "name":"net_out_size",
                "expression":"value/1024",
                "tag":"iface"
            }
        }
    }
}
